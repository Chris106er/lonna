{
  "name": "lonna",
  "version": "0.8.3",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "babel-jest": "^26.3.0",
    "concurrently": "^5.3.0",
    "jest": "^26.4.0",
    "ts-jest": "^26.4.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4"
  },
  "dependencies": {
    "optics-ts": "^1.2.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "concurrently \"yarn build:esm\" \"yarn build:cjs\"",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc",
    "build:watch": "tsc-watch --onSuccess \"yarn build:esm\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepublishOnly": "rm -rf dist && yarn build"
  }
}
